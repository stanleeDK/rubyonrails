<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<%= will_paginate  %>
	<ul class="users">
	  <% @users.each do |user| %>
	    <li>
	      <%= link_to user.name, user %> <%= user.email %>
	      <%= link_to 'Edit', edit_user_path(user) %>
	      <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
	    </li>
	  <% end %>
	</ul>


<br>

<%= link_to 'New User', new_user_path %>
